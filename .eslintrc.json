{
  "extends": [
    "airbnb-base",
    "plugin:node/recommended",
    "plugin:prettier/recommended"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "no-console": "off",
    "no-unused-vars": "warn",
    "prettier/prettier": ["error", { "singleQuote": false, "semi": true }]
  },
  "plugins": ["import", "node", "prettier"]
}
